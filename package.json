{
  "name": "nextbook",
  "version": "0.1.0",
  "description": "nextbook: epub3 author",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^5.0.3",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "gulp": "^3.8.7",
    "gulp-browserify": "^0.5.1",
    "gulp-connect": "^2.2.0",
    "gulp-gh-pages": "^0.4.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.3.3",
    "gulp-util": "^3.0.4",
    "reactify": "^1.0.0",
    "require-dir": "^0.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.4.0"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "dependencies": {
    "flux": "^2.0.1",
    "material-ui": "^0.7.0",
    "material-ui-sass": "^0.5.3",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-tap-event-plugin": "^0.1.4"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "multisortable": "./bower_components/jquery-multisortable/lib/jquery.multisortable.js",
    "jquery-ui": "./bower_components/jquery-ui/jquery-ui.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "multisortable": {
      "depends": [
        "jquery",
        "jquery-ui"
      ],
      "multisortable": null
    },
    "jquery-ui": {
      "depends": "jquery",
      "jquery-ui": null
    }
  }
}
